import BWTGeneralGraph;

n := 4;
def main(){
    weld := [4,13,6,14,5,11,7,12] coerce !uint[n]^(2^3);
    out := array(15, array(4, 0:!uint[n]));
    for a in [1..15){
        for col in [0..4){
            nd := a as uint[n];
            x := BWTOracle[n, 3](weld, col, nd);
            out[a][col] = measure(x);
            print(a);
        }
    }
    return out;
}